      "source": [
        "while True:\n",
        "  user_number = float(input('Enter your first number '))\n",
        "  user_op = input('Enter your operator ')\n",
        "  user_number2 = float(input('Enter your second number '))\n",
        "\n",
        "  if user_op == '+':\n",
        "    print('Your answer is:', user_number + user_number2)\n",
        "\n",
        "  elif user_op == '-':\n",
        "    print('Your answer is:', user_number - user_number2)\n",
        "\n",
        "  elif user_op == '*':\n",
        "    print('Your answer is:', user_number * user_number2)\n",
        "\n",
        "  elif user_op == '/':\n",
        "    print('Your answer is:', user_number / user_number2)\n",
        "\n",
        "  elif user_op == '//':\n",
        "    print('Your answer is:', user_number // user_number2)\n",
        "\n",
        "  elif user_op == '%':\n",
        "    print('Your answer is:', user_number % user_number2)\n",
        "\n",
        "  elif user_op == '**':\n",
        "    print('Your answer is:', user_number ** user_number2)\n",
        "\n",
        "  elif user_op == 'break':\n",
        "    break\n",
        "\n",
        "  else:\n",
        "    continue\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n"
