        "y = 4\n",
        "z = 6\n",
        "\n",
        "if x%2 != 0 and y%2 != 0 and z%2 != 0:\n",
        "    print(max(x, y, z))\n",
        "elif x%2 != 0 and y%2 != 0 and z%2 == 0:\n",
        "    print(max(x, y))\n",
        "elif x%2 != 0 and y%2 == 0 and z%2 != 0:\n",
        "    print(max(x, z))\n",
        "elif x%2 == 0 and y%2 != 0 and z%2 != 0:\n",
        "    print(max(y, z))\n",
        "elif x%2 != 0 and y%2 == 0 and z%2 == 0:\n",
        "    print(x)\n",
        "elif x%2 == 0 and y%2 != 0 and z%2 == 0:\n",
        "    print(y)\n",
        "elif x%2 == 0 and y%2 == 0 and z%2 != 0:\n",
        "    print(z)\n",
        "else:\n",
        "    print(min(x, y, z))"
